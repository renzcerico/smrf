[alias]
  co = checkout
  ci = commit
  st = status
  s = status -s
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  cmp = "!f() { git add -A && git commit -m \"$@\" && git push; }; f"
